{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run FrontApp",
      "type": "npm",
      "script": "dev",
      "isBackground": true,
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "never",
        "focus": true,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
      },
      "problemMatcher": {
        "fileLocation": "autoDetect",
        "pattern": {
          "regexp": "\\[vite\\] hmr (\\w+) (.+)",
          "message": 1,
          "file": 2
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": "VITE v\\d+\\.\\d+\\.\\d+",
          "endsPattern": "press h \\+ enter to show help"
        }
      }
    }
  ]
}
